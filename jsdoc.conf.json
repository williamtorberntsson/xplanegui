{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "src",
      "Documentation.md"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component",
    "node_modules/better-docs/category"
  ],
  "templates": {
    "better-docs": {
      "name": "XplaneGUI"
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "Documentation.md",
    "template": "node_modules/better-docs"
  }
}